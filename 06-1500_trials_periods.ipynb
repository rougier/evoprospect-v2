    "data_elo['subject_id'] = data_elo['subject_id'].replace(remplacement)\n",
    "\n",
    "\n",
    "# Date convertion\n",
    "data_elo['Date'] = pd.to_datetime(data_elo['Date'], format='%d/%m/%Y')\n"
